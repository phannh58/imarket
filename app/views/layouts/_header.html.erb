<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <%= link_to t("layout.header.name_app"), '#', id: "logo" %>
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <% if user_signed_in? %>
          <li><%= link_to t("layout.header.home"), static_pages_home_path %></li>
          <li><%= link_to t("layout.header.logout"), destroy_user_session_path %></li>
        <% else %>
          <li><%= link_to t("layout.header.login"), new_user_session_path %></li>
          <li><%= link_to t("layout.header.signup"), new_user_registration_path %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>
